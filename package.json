{
  "name": "assessment-editor",
  "version": "0.0.1",
  "author": "Jeff Schofield <jeff@jeffschofield.com>",
  "scripts": {
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "format": "eslint --fix \"**/*.{js,ts,tsx}\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "esno scripts/verify-commit.ts"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@leeoniya/ufuzzy": "^0.7.0",
    "@reduxjs/toolkit": "^1.8.6",
    "konva": "^8.3.13",
    "nanoid": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.2",
    "react-redux": "^8.0.4",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.31.8",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "jsdom": "^20.0.1",
    "kolorist": "^1.6.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svgson": "^5.2.1",
    "tailwindcss": "^3.1.8",
    "tailwindcss-gridlines": "^0.2.4",
    "tailwindcss-interaction-media": "^0.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.23.4",
    "yorkie": "^2.0.0"
  }
}